<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/grade_list_item_layout">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/grade_list_item_module_title"
        android:background="#c9e0fa"
        android:autoText="false"
        android:elegantTextHeight="false"
        android:singleLine="true"
        android:layout_alignTop="@+id/grade_list_item_module_grade"
        android:layout_alignRight="@+id/grade_list_item_module_grade"
        android:layout_alignEnd="@+id/grade_list_item_module_grade"
        android:layout_marginRight="109dp"
        android:layout_marginEnd="109dp"
        android:text="B-SQL INVIDVIDUAL TEST"
        android:textSize="16sp"
        android:layout_alignBottom="@+id/grade_list_item_module_grade"
        android:gravity="center_vertical"
        android:textColor="#3e3a3a"
         />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/grade_list_item_module_grade"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="249dp"
        android:layout_marginStart="249dp"
        android:background="#b5c2eb"
        android:layout_marginTop="2dp"
        android:layout_marginRight="3dp"
        android:gravity="bottom|center"
        android:text="Acquis"
        android:textSize="15dp"
        android:textColor="#3e3a3a" />
</RelativeLayout>
